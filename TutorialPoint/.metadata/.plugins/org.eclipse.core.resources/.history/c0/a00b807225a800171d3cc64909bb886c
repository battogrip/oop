package learnJava;

class FreshJuiceTest_Enum {
enum FreshJuiceSize{ SMALL, MEDIUM, LARGE }
FreshJuiceSize size;
}
public class FreshJuiceTest_Enum {
public static void main(String args[]){
FreshJuiceTest_Enum juice = new FreshJuiceTest_Enum();
juice.size = FreshJuiceTest_Enum.FreshJuiceSize.MEDIUM ;
System.out.println("Size: " + juice.size);
}
}